{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/termos",
        "destination": "/termos/index.html"
      },
      {
        "source": "/politicas",
        "destination": "/politicas/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
